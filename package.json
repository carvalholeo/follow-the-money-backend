{
  "name": "follow-the-money",
  "version": "1.0.0",
  "description": "Package made in TypeScript, using Express, Node, and Knex, for personal financial control.",
  "main": "dist/src/server.js",
  "scripts": {
    "test": "jest --coverage --no-cache",
    "prestart": "npm run knex:latest",
    "start": "node dist/src/server.js",
    "predev": "npm run knex:latest",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "build": "tsc --removecomments --types --outDir dist/",
    "postbuild": "cpy .env.example dist --dot --parents && cp dist/.env.example dist/.env",
    "sonar": "node ./bin/sonar",
    "key:generate": "node ./bin/keyGenerate",
    "knex:latest": "knex --knexfile knexfile.ts migrate:latest",
    "knex:rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "knex:seed": "knex --knexfile knexfile.ts seed:run",
    "lint": "npx eslint ./src"
  },
  "keywords": [
    "financial control",
    "money",
    "api"
  ],
  "author": "Leo Carvalho",
  "license": "AGPL-3.0-only",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.5",
    "log4js": "^6.3.0",
    "morgan": "^1.10.0",
    "mssql": "^6.2.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "oracledb": "^5.0.0",
    "pg": "^8.3.2",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/hapi__joi": "^17.1.4",
    "@types/helmet": "0.0.48",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^7.0.2",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.0",
    "@types/speakeasy": "^2.0.5",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.2",
    "del-cli": "^3.0.1",
    "eslint": "^7.7.0",
    "jest": "^26.4.1",
    "sonarqube-scanner": "^2.7.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.60",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": ">=12.x"
  }
}
